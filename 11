package company;
import rate.*;
public interface Company {
     public Rate producerate();
}

package company;
import rate.*;
public class DianximCompany implements Company{

	@Override
	public Rate producerate() {
		// TODO Auto-generated method stub
		return new DianxinRate();
	}
   
}
